@model BookMyShow.Models.Customer
@{
    ViewBag.Title = "Model Name";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Id</h2>
<h2>@Model.Name</h2>

<ul>
    <li>Membership Type: @Model.MembershipType.Name</li>
    @*<li>@Html.DisplayNameFor(m => m.MembershipType.Name) : @Model.MembershipType.Name</li>*@
    @if(Model.BirthDate.HasValue)
    {
        <li>Birthdate is: @Model.BirthDate.Value.ToShortDateString()</li>
    }
</ul>

